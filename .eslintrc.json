{
  "extends": "react-app",
  "rules": {
    "indent": ["error", 2],
    "semi": ["error", "never", { "beforeStatementContinuationChars": "never" }],
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "jsx-quotes": ["error", "prefer-single"],
    "eol-last": ["error", "always"]
  }
}
